{# templates/event/list.html.twig #}

{% extends 'base.html.twig' %}

{% block title %}Liste des événements
{% endblock %}

{% block body %}
	<h1>Liste des événements</h1>
	<ul>
		{% for event in events %}
			<li>{{ event.title }}
				-
				{{ event.date|date('d/m/Y H:i') }}</li>
		{% endfor %}
	</ul>
{% endblock %}

